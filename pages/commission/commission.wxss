page:after {
    content: " ";
    position: fixed;
    left: 0;
    top: 0;
    right: 0;
    height: 1rpx;
    border-top: 500rpx solid #4EAE7B;
    z-index: 10;
}

page {
    background: #f8f8f8;
}

.container {
    z-index: 100;
    position: relative;
    background: #fff;
}

.userinfo {
    flex-direction: column;
    align-items: center;
    position: relative;
    text-align: center;
    padding-top: 50rpx;
}

.userinfo-face {
    width: 160rpx;
    height: 160rpx;
    display: block;
    border-radius: 50%;
    border: 4rpx solid rgba(0,0,0,0.1);
    margin: 0rpx auto 30rpx auto;
}

.userinfo-name {
    color: #fff;
    line-height: 50rpx;
    font-size: 32rpx;
}

.userinfo-level {
    color: #FFE10F;
    margin-left: 10rpx;
}

.userinfo-time {
    color: #fff;
    line-height: 50rpx;
    font-size: 22rpx;
}

.userinfo-recharge {
    padding-top: 20rpx;
}

.userinfo-recharge text {
    border-radius: 8rpx;
    line-height: 80rpx;
    font-size: 28rpx;
    color: #fff;
    border: 1rpx solid #fff;
    display: inline-block;
    padding: 0rpx 100rpx;
}

.user-balance {
    display: flex;
    padding: 30rpx 0rpx;
    margin-left: -1rpx;
    margin-top: 20rpx;
    width: 751rpx;
    background: rgba(0,0,0,0.1);
}

.user-balance .li {
    border-left: 1rpx solid rgba(255,255,255,0.2);
    box-sizing: border-box;
    flex: 1;
    text-align: center;
}

.user-balance .li:first-child {
    border-left-width: 0rpx;
}

.user-balance .t1 {
    font-size:28rpx;
    color: #fff;
}
.user-balance .t1 .ft{
    font-size:32rpx;
    margin-bottom:20rpx;
}

.user-balance .t2 {
    padding-top: 20rpx;
    color: #fff;
    opacity: 0.8;
}

.become1_is_show {
    width: 750rpx;
    height: 100%;
    background: #f00;
}

.become {
    padding: 20rpx;
    border-bottom: 1rpx solid #d9d9d9;
}

.become .text {
    line-height: 40rpx;
    padding-bottom: 20rpx;
    font-size: 28rpx;
}

.become .but {
    height: 88rpx;
    line-height: 88rpx;
    border-radius: 4rpx;
    text-align: center;
    font-size: 28rpx;
}

.privilege {
    background: #fff;
}

.privilege .head {
    border-top: 1rpx solid #d9d9d9;
    line-height: 80rpx;
    padding: 0rpx 20rpx;
    border-bottom: 1rpx solid #d9d9d9;
}

.privilege .main {
}

.privilege .li {
    border-bottom: 1rpx solid #d9d9d9;
    display: flex;
    padding: 20rpx 20rpx;
}

.privilege .image {
    height: 80rpx;
    width: 80rpx;
}

.privilege .text {
    flex: 1;
    padding-left: 20rpx;
}

.privilege .t1 {
    color: #555;
    font-size: 28rpx;
}

.privilege .t2 {
    color: #888;
}

.privilege .t3 {
    color: #888;
}

.register {
    border-bottom: 1rpx solid #d9d9d9;
}

.register .input {
    padding: 0rpx 25rpx;
}

.register .input .i1 {
    background: #f8f8f8;
    line-height: 88rpx;
    height: 88rpx;
    padding: 0rpx 20rpx;
    margin-top: 20rpx;
    font-size: 28rpx;
}

.imagesTitle {
    width: 750rpx;
    height: 240rpx;
}

.altText {
    padding: 30rpx 0rpx 10rpx 0rpx;
    text-align: center;
    line-height: 65rpx;
}

.altText .te1 {
    font-size: 36rpx;
    color: #414141;
}

.altText .te2 {
    font-size: 28rpx;
    color: #777;
}


.tuij-list{
    display: flex;
    display:-webkit-flex;
    flex-wrap:wrap;
    overflow: hidden;
}
.tuij-list .tj-lt{
   width:44%;
   float: left;
   border-right:1px solid #d6d6d6;
   border-bottom:1px solid #d6d6d6;
   padding:35rpx 2% 40rpx 4%;
   display: flex;
   font-size:28rpx;
   color: #666;
   margin-left:-1px;
}
.tuij-list .title{
  font-size: 30rpx;
  color: #333;
  margin-bottom: 15rpx;
}
.tuij-list .num{
  color: #f00;
}
.tuij-list .tj-lt image{
  width:60rpx;
  height:60rpx;
  float: left;
  display: block;
  margin-top:15rpx;
}
.tuij-list .lt-text{
  float: left;
  margin-left:30rpx;
}












