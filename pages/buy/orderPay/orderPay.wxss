page {
    background: #f8f8f8;
}

.inputBox {
    background: #fff;
    padding: 0rpx 20rpx;
    border-bottom: 1rpx solid #D9D9D9;
    overflow: hidden;
    margin-top: -3rpx;
}

.inputBox .li {
    border-top: 1rpx solid #d9d9d9;
    margin: 0rpx;
    display: flex;
    flex-flow: nowrap;
    align-items: center;
    padding: 10rpx 0rpx;
    font-size: 28rpx;
    height: 70rpx;
    overflow: hidden;
}

.inputBox .li .ll {
    flex: 1;
}

.inputBox .li .t1 {
    margin-right: 10rpx;
}

.inputBox .li .t2 {
    margin-right: 10rpx;
    color: #f22;
}

.inputBox .li .rr {
    text-align: right;
    flex: 2;
}

.inputBox .icon {
    height: 30rpx;
    display: block;
    width: 30rpx;
}
.credit button{
  background: red;
}
